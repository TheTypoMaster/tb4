{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.2.*",
		"guzzlehttp/guzzle": "~5.1",
		"pda/pheanstalk": "~2.1"
	},
	"require-dev": {
	  "way/generators": "1.*",
	  "barryvdh/laravel-ide-helper": "1.*",
	  "barryvdh/laravel-migration-generator": "dev-master",
	  "orangehill/iseed": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/topbetta/models",
			"app/topbetta/api/backend/controllers",
			"app/topbetta/api/backend/models",
			"app/topbetta/api/frontend/controllers",
			"app/topbetta/api/frontend/models",
			"app/topbetta/api/admin/controllers",
			"app/topbetta/api/libraries",
			"app/topbetta/libraries/wagering",
			"app/topbetta/helpers",
            "app/topbetta/providers",
            "app/topbetta/repositories",
            "app/topbetta/facades"
		],
        "psr-4": {
            "TopBetta\\": "app/topbetta"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
        ],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"

}
